html {
    overflow-x: auto;
    overflow-y: scroll;
    display: block;
    padding: 0;
    font-size: .8em;
    line-height: 1.5;
}

body.mikio {
    display: flex;
    flex-direction: column;
    min-height: 100vh;
    font-family: -apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Arial,"Noto Sans",sans-serif;
    padding: 0 1rem;
}

html, body.mikio {
    background-color: #fbfaf9;
    color: #333;
    margin: 0;
}

/* Navbar */
.mikio-navbar {
    position: relative;
    display: flex;
    padding: 1rem 2rem;
    margin-bottom: 0.5rem;
}

.mikio-navbar-brand {
    display: flex;
    align-items: center;
    text-decoration: none;
}

.mikio-navbar-brand-image {
    width: 4rem;
    height: 4rem;
    margin-right: 1rem;
}

.mikio-navbar-brand-title-text {
    font-size: 1.75rem;
    color: #333;
}

.mikio-navbar-brand-title-tagline {
    font-size: 1rem;
    color: #333;
    margin-top: -.3rem;
}

.mikio-navbar-toggle {
    display: none;
    width: 3rem;
    height: 3rem;
    border: 1px solid #ddd;
    border-radius: .25rem;
    fill: #999;
    overflow: hidden;
    padding: 0rem .5rem;
}

.mikio-navbar-collapse {
    display: flex;
    flex: 1;
    align-items: center;
    justify-content: flex-end;
}


/* Nav */
.mikio-nav {
    list-style: none;
    margin: 0;
    padding: 0;
}

.mikio-nav-item {
    margin: 0 1rem;
}

.mikio-nav-item:first-of-type {
    margin-left: 0;
}

.mikio-nav-item:last-of-type {
    margin-right: 0;
}

.mikio-nav-dropdown {
    padding-right: 1.5rem;
    background-size: 1.2rem;
    background-position: right;
    background-repeat: no-repeat;
    background-image: url('data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciIHdpZHRoPSIzMiIgaGVpZ2h0PSIzMiIgdmlld0JveD0iMCAwIDMyIDMyIj48cGF0aCBkPSJNMTYuMDAzIDE4LjYyNmw3LjA4MS03LjA4MUwyNSAxMy40NmwtOC45OTcgOC45OTgtOS4wMDMtOSAxLjkxNy0xLjkxNnoiLz48L3N2Zz4=');
}

.mikio-nav-link {
    text-decoration: none;
    color: #333;
}

.mikio-dropdown {
    display: none;
    position: absolute;
    z-index: 1000;
    right: 0;
    bottom: auto;

    background-color: #f9f9f9;
    min-width: 160px;
    box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
    padding: .5rem 0;
    transition: all .15s ease-in-out;
}

.mikio-nav-dropdown:hover .mikio-dropdown {
    display: block;
}

.mikio-dropdown-header {
    margin: 0;
    padding: 1rem 2rem;
    font-size: 1rem;
}

.mikio-dropdown-item {
    display: flex;
    align-items: center;
    padding: .25rem 2rem;
    transition: all .15s ease-in-out;
}

.mikio-dropdown-item:hover {
    background-color: #eee;
}

.mikio-dropdown-divider {
    margin: .5rem 0 .5rem 0;
    border-bottom: 1px solid rgba(0,0,0,0.2);
}


/* Breadcrumbs */
.mikio-breadcrumbs {
    padding: .5rem 2rem;
    border-width: 1px 0 1px 0;
    border-style: solid;
    border-color: #ccc;
    margin-bottom: 1rem;
}


/* Page */
.mikio-page {
    display: flex;
}


/* Sidebar */
.mikio-sidebar {
    margin: 0 1rem;
    flex: 0 0 12rem;
}

.mikio-sidebar ul {
    padding-left: 1rem;
}

.mikio-sidebar ul ul {
    padding-left: .5rem;
}

.mikio-sidebar-toggle {
    display: none;
    background-size: 1rem;
    background-position: 95% 50%;
    background-repeat: no-repeat;
    background-image: url('data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciIHdpZHRoPSIzMiIgaGVpZ2h0PSIzMiIgdmlld0JveD0iMCAwIDMyIDMyIj48cGF0aCBkPSJNMTYuMDAzIDE4LjYyNmw3LjA4MS03LjA4MUwyNSAxMy40NmwtOC45OTcgOC45OTgtOS4wMDMtOSAxLjkxNy0xLjkxNnoiLz48L3N2Zz4=');
}

.mikio-sidebar-toggle.closed {
    background-image: url('data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciIHdpZHRoPSIzMiIgaGVpZ2h0PSIzMiIgdmlld0JveD0iMCAwIDMyIDMyIj48cGF0aCBkPSJNMTUuOTk3IDEzLjM3NGwtNy4wODEgNy4wODFMNyAxOC41NGw4Ljk5Ny04Ljk5OCA5LjAwMyA5LTEuOTE2IDEuOTE2eiIvPjwvc3ZnPg==');
}


/* Content */
.mikio-content {
    margin: 0 1rem;
    flex: 1 1 auto;
}

.mikio-content > .mikio-breadcrumbs {
    padding-left: 1rem;
    padding-right: 1rem;
}


/* Article */
.mikio-article {
    background-color: #fff;
    border: 1px solid #eee;
    box-shadow: 0 0 .5em #999;
    padding: 2rem;
}

.mikio-article > *:nth-child(2) {
    margin-top: 0;
    word-break: normal;
}


/* TOC */
.mikio-toc {
    float: right;
    border: 1px solid #ddd;
    max-width: 20rem;
    margin-left: 1.5rem;
    margin-bottom: 1.5rem;
}

.mikio-toc h3 {
    margin: 0;
    padding: .25rem .5rem;
    font-size: 100%;
    text-align: center;

    background-color: #ddd;
    background-size: 1rem;
    background-position: 95% 50%;
    background-repeat: no-repeat;
    background-image: url('data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciIHdpZHRoPSIzMiIgaGVpZ2h0PSIzMiIgdmlld0JveD0iMCAwIDMyIDMyIj48cGF0aCBkPSJNMTYuMDAzIDE4LjYyNmw3LjA4MS03LjA4MUwyNSAxMy40NmwtOC45OTcgOC45OTgtOS4wMDMtOSAxLjkxNy0xLjkxNnoiLz48L3N2Zz4=');
}

.mikio-toc h3:before {
    display: inline-block;
    content: "";
    width: 1rem;
    height: 1rem;
    margin-right: .5rem;
    background-size: 1rem;
    background-position: center;
    background-repeat: no-repeat;
    background-image: url('data:image/svg+xml;base64,');
}

.mikio-toc h3.closed {
    margin-bottom: 0;
    background-image: url('data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciIHdpZHRoPSIzMiIgaGVpZ2h0PSIzMiIgdmlld0JveD0iMCAwIDMyIDMyIj48cGF0aCBkPSJNMTUuOTk3IDEzLjM3NGwtNy4wODEgNy4wODFMNyAxOC41NGw4Ljk5Ny04Ljk5OCA5LjAwMyA5LTEuOTE2IDEuOTE2eiIvPjwvc3ZnPg==');
    font-size: 0;
}

.mikio-toc h3.closed:before {
    margin-right: 1rem;
}

.mikio-toc h3 span {
    display: none;
}


.mikio-toc ul {
    padding-right: 1.5rem;
}

.mikio-toc ul ul {
    padding-left: 1rem;
}


/* Page Tools */
#dw__pagetools {
    position: -webkit-sticky;
    position: sticky;
    top: 0;
    align-self: flex-start;
    margin-top: 4rem;
    padding-top: 2rem;
    margin-right: 1rem;
}

#dw__pagetools a {
    display: block;
}

#dw__pagetools a svg {
    fill: #ccc;
    transition: all .15s ease-in-out;
}

#dw__pagetools a:hover svg {
    fill: #000;
}


/* Footer */
.mikio-footer {
    margin-top: 2rem;
    padding: 0 1rem;
    text-align: center;
}


/* Search */
.mikio-search {
    display: flex;
}

.mikio-search input {
    margin-right: 0;
    border-right-width: 0;
    border-radius: .25rem 0 0 .25rem;
    width: 1%;
    flex: 1 1 auto;
}

.mikio-search button[type=submit] {
    margin-left: 0;
    border-radius: 0 .25rem .25rem 0;
    transition: all .15s ease-in-out;
    color: #333;
    border-color: #ced4da;
    background-color: #eee;
}

.mikio-search button[type=submit]:hover {
    background-color: #ced4da;
    color: inherit;
}


/* Icon */
.mikio-icon {
    margin-right: .5rem;
}

.mikio-icon:nth-last-child(1) {
    margin-left: .5rem;
}


/* Input */
input[type=search] {
    -webkit-appearance: none;
}

select {
    -webkit-appearance: none;
    background-size: 1rem;
    background-position: 98% 50%;
    background-repeat: no-repeat;
    background-image: url('data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciIHdpZHRoPSIzMiIgaGVpZ2h0PSIzMiIgdmlld0JveD0iMCAwIDMyIDMyIj48cGF0aCBkPSJNMjQgMTEuMzA1bC03Ljk5NyAxMS4zOUw4IDExLjMwNXoiLz48L3N2Zz4=');
}

