/* Admin */
.mikio-act-admin .ui-admin .admin_tasks, .mikio-act-admin .ui-admin .admin_plugins {
    list-style: none;
    padding: 0;
    display: grid;
    grid-template-columns: repeat(auto-fit, 17rem);
}

.mikio-act-admin .ui-admin .admin_tasks li, .mikio-act-admin .ui-admin .admin_plugins li {
    border: 1px solid #ced4da;
    border-radius: .25rem;
    margin: 1rem;
}

.mikio-act-admin .ui-admin .admin_tasks li a, .mikio-act-admin .ui-admin .admin_plugins li a {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    text-align: center;
    height: 6rem;
    padding: 1rem 2rem;
    color: #333;
    text-decoration: none;
    transition: all .15s ease-in-out;
}

.mikio-act-admin .ui-admin .admin_tasks li a:hover, .mikio-act-admin .ui-admin .admin_plugins li a:hover {
    background-color: #ced4da;
}


/* Config Manager */
.mikio-act-admin #config__manager fieldset {
    margin: 0 0 2rem 0;
    padding: 0;
    background-color: transparent;
    border: 0;
}

.mikio-act-admin #config__manager fieldset legend {
    display: block;
    width: 100%;
    box-sizing: border-box;
    padding: .5rem 1rem;
    margin: 0;
    border-top: 1px solid #dee2e6;
    border-bottom: 1px solid #dee2e6;
    background-color: #454d55;
    color: #fff;
}

.mikio-act-admin #config__manager fieldset table {
    margin: 0;
}

.mikio-act-admin #config__manager fieldset .table td.label {
    display: grid;
    grid-template-columns: auto 32px;
    grid-template-rows: auto auto;
    align-items: center;
}

.mikio-act-admin #config__manager fieldset .table td.label .outkey {
    grid-column-start: 1;
    grid-column-end: span 1;
    grid-row-start: 1;
    grid-row-end: span 1;
    font-size: 100%;
    margin: 0;
    background-color: transparent;
    float: none;
}

.mikio-act-admin #config__manager fieldset .table td.label label {
    grid-column-start: 1;
    grid-column-end: span 1;
    grid-row-start: 2;
    grid-row-end: span 1;
}

.mikio-act-admin #config__manager fieldset .table td.label img {
    grid-column-start: 2;
    grid-column-end: span 1;
    grid-row-start: 1;
    grid-row-end: span 2;
}

.mikio-act-admin #config__manager fieldset .table td.value div.input {
    background-color: transparent;
}

#edit__summary, .mikio-act-admin input[type=text], .mikio-act-admin input[type=password], .mikio-act-admin input[type=email], .mikio-act-admin select {
    box-sizing: border-box;
    border: 1px solid #ced4da;
    line-height: 1.5;
    padding: .375rem .75rem;
    font-size: 1rem;
    border-radius: .25rem;
    margin: 0;
}

.mikio-act-admin td input[type=text], .mikio-act-admin td input[type=password], .mikio-act-admin td input[type=email], .mikio-act-admin #config__manager input[type=text], .mikio-act-admin #config__manager input[type=password], .mikio-act-admin #config__manager input[type=email], .mikio-act-admin #config__manager input[type=file], .mikio-act-admin #config__manager select {
    width: 100%;
}

.mikio-act-admin input[type=file] {
    width: auto;
    position: relative;
}


.mikio-act-admin #config__manager tr.default .input, .mikio-act-admin #config__manager tr.default input, .mikio-act-admin #config__manager tr.default textarea, .mikio-act-admin #config__manager tr.default select, .mikio-act-admin #config__manager .selectiondefault {
    background-color: rgba(204, 221, 255, .5);
}

.mikio-act-admin #config__manager tr.protected .input, .mikio-act-admin #config__manager tr.protected input, .mikio-act-admin #config__manager tr.protected textarea, .mikio-act-admin #config__manager tr.protected select, .mikio-act-admin #config__manager tr.protected .selection {
    background-color: rgba(255, 204, 204, .5) !important;
}

.mikio-act-admin ul.tabs {
    list-style: none;
    padding-left: 1rem;
    border-bottom: 1px solid #dee2e6;
}

.mikio-act-admin ul.tabs li {
    display: inline-block;
}

.mikio-act-admin ul.tabs li a {
    display: block;
    padding: .5rem 1rem;
    border: 1px solid transparent;
    border-radius: .25rem .25rem 0 0;
    text-decoration: none;
    color: #007bff;
    transition: all .15s ease-in-out;
}

.mikio-act-admin ul.tabs li a:hover {
    border-color: #e9ecef #e9ecef #dee2e6;
}

.mikio-act-admin ul.tabs li.active a, .mikio-act-admin #extension__manager ul.tabs li.active a {
    color: #333;
    background-color: #fff;
    border-color: #dee2e6 #dee2e6 #fff;
}


/* Dokuwiki Theme Fixes */
button {
    /* margin-left: 0;
    margin-right: 0; */
}

label {
    /* position: relative; */
    display: inline-block;
}

label.block {
    display: block;
}

.tool__bar button {
    margin: 0 0 1rem 0;
    border-radius: 0;
    border-right-width: 0;
}

.tool__bar button:first-of-type {
    border-radius: .25rem 0 0 .25rem;
}

.tool__bar button:last-of-type {
    border-radius: 0 .25rem .25rem 0;
    border-right-width: 1px;
}

.tool__bar button:hover {
    background-color: #ced4da;
}

.tool__bar button img {
    vertical-align: middle;
}

.mikio-article .summary {
    margin: 1rem 0;
}

/* Table */
.mikio-article .table table {
    width: 100%;
    color: #212529;
    border-collapse: collapse;
}

.mikio-article .table th, .table td {
    padding: .5rem 1rem;
}

.mikio-article .table td {
    border-top: 1px solid #dee2e6;
}

.mikio-article .table tbody tr:nth-child(odd) {
    background-color: rgba(0, 0, 0, .05);
}

.mikio-article .table caption + thead tr:first-child th, .mikio-article .table caption + thead tr:first-child td, .mikio-article .table colgroup + thead tr:first-child th, .mikio-article .table colgroup + thead tr:first-child td, .mikio-article .table thead:first-child tr:first-child th, .mikio-article .table thead:first-child tr:first-child td {
    border-top: 1px solid #dee2e6;
}

.mikio-article .table caption + thead tr:last-child th, .mikio-article .table caption + thead tr:last-child td, .mikio-article .table colgroup + thead tr:last-child th, .mikio-article .table colgroup + thead tr:last-child td, .mikio-article .table thead:last-of-type tr:last-child th, .mikio-article .table thead:last-of-type tr:last-child td {
    border-bottom: 2px solid #dee2e6;
}

.mikio-article .inclmeta {
    color: #999;
    text-align: right;
}

.mikio-article .secedit {
    text-align: right;
}

.mikio #acl__tree {
    padding: .5rem 0;
    font-size: 100%;
}


/* Mikio Input File */
.mikio-input-file {
    position: absolute;
    /* margin-top: -.5rem; */
    top: 0;
    right: 0;
    left: 0;
    z-index: 5;
    padding: .25rem 1rem;
    line-height: 1.5;
    text-align: left;
    color: #555;
    background-color: #fff;
    border: .1px solid #ced4da;
    border-radius: .25rem;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;    
    white-space: nowrap; 
    overflow: hidden;
    text-overflow: ellipsis;
    box-sizing: border-box;
}

.mikio-input-file:before {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    z-index: 6;
    display: block;
    content: "Browse";
    padding: .25rem 1rem;
    line-height: 1.5;
    color: #555;
    background-color: #eee;
    border: .075rem solid #ddd;
    border-radius: 0 .25rem .25rem 0;
    transition: all .15s ease-in-out;
}

.mikio-input-file:hover:before {
    background-color: #ced4da;
}


/* Responsive */
@media (max-width: 768px) {
    .mikio-navbar {
        display: grid;
        grid-template-columns: auto 3rem;
        grid-template-rows: auto auto;
        padding: 1rem 0;
    }

    .mikio-navbar-brand {
        
    }

    .mikio-navbar-brand-title {
        grid-column-start: 1;
        grid-column-end: span 1;
        grid-row-start: 1;
        grid-row-end: span 1;
        flex: 1;
    }

    .mikio-navbar-toggle {
        grid-column-start: 2;
        grid-column-end: span 1;
        grid-row-start: 1;
        grid-row-end: span 1;
        display: block;
        align-self: right;
    }

    .mikio-navbar-collapse {
        grid-column-start: 1;
        grid-column-end: span 2;
        grid-row-start: 2;
        grid-row-end: span 1;
        flex-direction: column;
    }

    .mikio-navbar-toggle.closed + .mikio-navbar-collapse {
        display: none;
        margin: 0 -1rem 0 -1rem;
    }

    .mikio-nav-item {
        width: 100%;
    }

    .mikio-nav-dropdown {
        background: none;
        padding-right: 0;
    }

    .mikio-nav-dropdown .mikio-dropdown {
        display: flex;
        flex-direction: column;
        position: relative;
    }

    .mikio-dropdown {
        display: flex;
        box-shadow: none;
        /* max-width: none; */
    }

    .mikio-page {
        display: grid;
        grid-template-columns: auto 2rem;
        grid-template-rows: auto auto auto;
        /* flex-direction: column; */
    }

    .mikio-sidebar {
        grid-column-start: 1;
        grid-column-end: span 2;
        grid-row-start: 1;
        grid-row-end: span 1;
    }

    .mikio-article {
        padding: .5rem;
        margin: 0 -1rem;
        box-shadow: none;
    }

    .mikio-toc {
        float: none;
        display: block;
        max-width: none;
        margin-left: 0;
    }

    .mikio-toc h3.closed {
        font-size: 100%;
    }
    

    .mikio-sidebar-toggle {
        display: block;
    }

    .mikio-sidebar-toggle.closed + .mikio-sidebar-collapse {
        display: none;
    }    
}





/* Mikio Elements */
.mikio-alert {
    position: relative;
    padding: .75rem 1.25rem;
    margin-bottom: 1rem;
    border: 1px solid transparent;
    border-radius: .25rem;
}

.mikio-button {
    background-color: #fff;
    border: 1px solid #6c757d;
    border-radius: .25rem;
    color: #6c757d;
    padding: .375rem .75rem;
    font-size: 1rem;
    line-height: 1.5;
    transition: all .15s ease-in-out;
}

.mikio-button:hover {
    color: #fff;
    background-color: #6c757d;
    border-color: #6c757d;
}


/* Mikio Styles */
.mikio-primary {
    color: #fff;
    background-color: #007bff;
    border-color: #007bff;
}

.mikio-primary:hover {
    background-color: #0069d9;
    border-color: #0062cc;
}

.mikio-alert.mikio-primary {
    color: #004085;
    background-color: #cce5ff;
